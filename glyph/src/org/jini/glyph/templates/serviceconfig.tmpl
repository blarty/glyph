import net.jini.discovery.LookupDiscovery;
import net.jini.lookup.entry.*;
import net.jini.core.entry.*;

$[packageName].$[className]{
	   initialGroups = $[group];
       initialLocators = null;
       initialAttributes = new Entry[]{new Name("$[svcName]")};
       exporterTypeName = "$[packageName].$[className]";
       confdir="$[currentdir]";
       exporterDefDir="$[currentdir]/conf/expdir";
	   persistentDirectory="$[currentdir]/log/$[svcName]";
} 	